{
  "PnPCore": {
    "DisableTelemetry": "true",
    "HttpRequests": {
      "UserAgent": "NONISV|SharePointPnP|PnPCoreSDK",
      "SharePointRest": {
        "UseRetryAfterHeader": "false",
        "MaxRetries": "10",
        "DelayInSeconds": "3",
        "UseIncrementalDelay": "true"
      },
      "MicrosoftGraph": {
        "UseRetryAfterHeader": "true",
        "MaxRetries": "10",
        "DelayInSeconds": "3",
        "UseIncrementalDelay": "true"
      }
    },
    "PnPContext": {
      "GraphFirst": "true",
      "GraphCanUseBeta": "true",
      "GraphAlwaysUseBeta": "false"
    },
    "Credentials": {
      "DefaultConfiguration": "MySetup",
      "Configurations": {
        "MySetup": {
          "CredentialManager": {
            "CredentialManagerName": "credman"
          }
        }
      }
    },
    "Sites": {
      "TestSite": {
        "SiteUrl": "https://contoso.sharepoint.com/sites/pnpcoresdktestgroup",
        "AuthenticationProviderName": "MySetup"
      },
      "TestSubSite": {
        "SiteUrl": "https://contoso.sharepoint.com/sites/pnpcoresdktestgroup/subsite",
        "AuthenticationProviderName": "MySetup"
      },
      "NoGroupTestSite": {
        "SiteUrl": "https://contoso.sharepoint.com/sites/pnpcoresdktest",
        "AuthenticationProviderName": "MySetup"
      },
      "ClassicSTS0TestSite": {
        "SiteUrl": "https://contoso.sharepoint.com/sites/sts0",
        "AuthenticationProviderName": "MySetup"
      },
      "TenantAdminCenterSite": {
        "SiteUrl": "https://contoso-admin.sharepoint.com",
        "AuthenticationProviderName": "MySetup"
      },
      "SiteCollectionAppCatalogSite": {
        "SiteUrl": "https://contoso.sharepoint.com/sites/dev-catalog",
        "AuthenticationProviderName": "MySetup"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  }
}
